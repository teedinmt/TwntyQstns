<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="twentyquestions.LoginPageController">
    <stylesheets>
        <URL value="@/css/loginpage.css" />
    </stylesheets>
       <children>
        <Button fx:id="Login_Button" layoutX="500.0" layoutY="486.0" onAction="#LoginButton" text="Staff Login">
         <font>
            <Font name="Arial Narrow Italic" size="14.0" />
         </font></Button>
        <TextField fx:id="UserNameTextField" layoutX="401.0" layoutY="433.0" prefHeight="25.0" prefWidth="106.0" />
        <TextField fx:id="PasswordTextField" layoutX="610.0" layoutY="433.0" prefHeight="25.0" prefWidth="113.0" />
        <Label fx:id="UserNameLabel" alignment="CENTER_RIGHT" layoutX="279.0" layoutY="435.0" prefHeight="22.0" prefWidth="113.0" text="User Name:">
         <font>
            <Font name="Arial Narrow" size="14.0" />
         </font></Label>
        <Label fx:id="PasswordLabel" alignment="CENTER_RIGHT" layoutX="512.0" layoutY="437.0" prefHeight="17.0" prefWidth="92.0" text="Password:">
         <font>
            <Font name="Arial Narrow" size="14.0" />
         </font></Label>
        <Label fx:id="textWelcome" layoutX="133.0" layoutY="45.0" text="Welcome!">
         <font>
            <Font name="Arial Narrow Bold Italic" size="64.0" />
         </font></Label>
      <Button fx:id="startnewapplication" alignment="CENTER" contentDisplay="CENTER" layoutX="265.0" layoutY="270.0" mnemonicParsing="false" onAction="#startNewApplication" prefHeight="53.0" prefWidth="469.0" text="Start A New Application">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font></Button>
      <Label fx:id="connectionNOTsuccessful" layoutX="278.0" layoutY="144.0" text="Alert!  The program encountered an error: " textFill="#d31010">
         <font>
            <Font name="Arial Narrow Bold Italic" size="24.0" />
         </font>
      </Label>
      <TextArea fx:id="customtextarea" layoutX="202.0" layoutY="187.0" prefHeight="68.0" prefWidth="551.0" wrapText="true">
         <font>
            <Font name="Arial Narrow Bold Italic" size="18.0" />
         </font>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></TextArea>
      <Button fx:id="exiterrorbutton" layoutX="461.0" layoutY="360.0" mnemonicParsing="false" onAction="#errorExitProgram" text="Exit">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Button>
      <Button fx:id="ExitProgramButton" layoutX="122.0" layoutY="538.0" mnemonicParsing="false" onAction="#exitProgram" text="Exit Program">
         <font>
            <Font name="Arial Narrow" size="18.0" />
         </font>
      </Button>
    </children>
    
    
</AnchorPane>
